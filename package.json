{
  "name": "formik-persist-values-v2",
  "description": "Fork of formik-persist-values with updated dependencies for React 18+ and modern ecosystem",
  "version": "1.0.2",
  "license": "MIT",
  "author": "Grigoriy Kolenko <kolenko@me.com> (original), Twensons (update)",
  "repository": "twensons/formik-persist-values-v2",
  "keywords": [
    "formik",
    "react",
    "react-dom",
    "form",
    "validation",
    "forms",
    "localstorage",
    "hoc"
  ],
  "main": "dist/PersistFormikValues.js",
  "module": "dist/PersistFormikValues.es6.js",
  "typings": "dist/PersistFormikValues.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest --env=jsdom",
    "test:watch": "npm run test -- --watch",
    "start": "cross-env NODE_ENV=development tsc-watch --onSuccess \"rollup -c\"",
    "prebuild": "rimraf dist",
    "build": "tsc && cross-env NODE_ENV=production rollup -c && rimraf compiled",
    "prepublish": "npm run build",
    "format": "prettier --trailing-comma es5 --single-quote --write 'src/**/*' 'test/**/*'",
    "precommit": "lint-staged",
    "addc": "all-contributors add",
    "gen-docs": "all-contributors generate && doctoc README.md"
  },
  "dependencies": {
    "lodash.omit": "^4.5.0",
    "react-use": "^17.6.0"
  },
  "peerDependencies": {
    "formik": ">=2.0.0",
    "react": ">=16"
  },
  "resolutions": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6"
  },
  "devDependencies": {
    "@types/jest": "24.0.25",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.omit": "^4.5.9",
    "@types/node": "^24.0.15",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "all-contributors-cli": "^6.26.1",
    "cross-env": "^7.0.3",
    "doctoc": "^1.3.0",
    "formik": "^2.4.6",
    "husky": "4.0.3",
    "jest": "24.9.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rimraf": "3.0.0",
    "rollup": "1.29.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-filesize": "6.2.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^5.1.3",
    "ts-jest": "24.3.0",
    "tsc-watch": "4.0.0",
    "tslint": "^5.20.1",
    "tslint-react": "4.1.0",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "semi": true
  }
}
